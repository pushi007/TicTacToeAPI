<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
	<changeSet author="rachna" id="20160414-01">
		<createTable tableName="QRTZ_CRON_TRIGGERS">
			<column name="SCHED_NAME" type="VARCHAR(100)" />
			<column name="TRIGGER_NAME" type="VARCHAR(100)" />
			<column name="TRIGGER_GROUP" type="VARCHAR(100)" />
			<column name="CRON_EXPRESSION" type="VARCHAR(50)" />
			<column name="TIME_ZONE_ID" type="VARCHAR(50)" />
		</createTable>
	</changeSet>
	<changeSet author="rachna" id="20160414-02">

		<createTable tableName="QRTZ_TRIGGERS">

			<column name="description" type="VARCHAR(250)" />

			<column name="sched_name" type="VARCHAR(50)" />
			<column name="job_group" type="VARCHAR(50)" />
			<column name="job_name" type="VARCHAR(50)" />
			<column name="trigger_name" type="VARCHAR(50)" />
			<column name="trigger_group" type="VARCHAR(50)" />
			<column name="trigger_type" type="VARCHAR(50)" />
			<column name="time_expression" type="VARCHAR(50)" />
			<column name="trigger_state" type="VARCHAR(50)" />
			<column name="group_name" type="VARCHAR(50)" />
			<column name="target_bean_name" type="VARCHAR(100)" />
			<column name="NEXT_FIRE_TIME" type="BIGINT" />
			<column name="PREV_FIRE_TIME" type="BIGINT" />
			<column name="PRIORITY" type="INT" />
			<column name="START_TIME" type="BIGINT" />
			<column name="END_TIME" type="BIGINT" />
			<column name="CALENDAR_NAME" type="VARCHAR(100)" />
			<column name="MISFIRE_INSTR" type="INT" />
			<column name="JOB_DATA" type="LONGBLOB" />


		</createTable>
	</changeSet>

	<changeSet author="rachna" id="20160414-03">
		<createTable tableName="QRTZ_LOCKS">
			<column name="sched_name" type="VARCHAR(50)" />
			<column name="LOCK_NAME" type="VARCHAR(50)" />
		</createTable>
	</changeSet>
	<changeSet author="rachna" id="20160414-04">

		<createTable tableName="QRTZ_JOB_DETAILS">
			<column name="JOB_GROUP" type="VARCHAR(50)" />
			<column name="JOB_NAME" type="VARCHAR(50)" />
			<column name="sched_name" type="VARCHAR(50)" />
			<column name="LOCK_NAME" type="VARCHAR(50)" />
			<column name="DESCRIPTION" type="VARCHAR(100)" />
			<column name="JOB_CLASS_NAME" type="VARCHAR(100)" />
			<column name="IS_DURABLE" type="BIT(1)" />

			<column name="IS_NONCONCURRENT" type="BIT(1)" />

			<column name="IS_UPDATE_DATA" type="BIT(1)" />

			<column name="REQUESTS_RECOVERY" type="BIT(1)" />
			<column name="JOB_DATA" type="LONGBLOB" />

		</createTable>
	</changeSet>
	<changeSet author="rachna" id="20160414-05">

		<createTable tableName="QRTZ_PAUSED_TRIGGER_GRPS">
			<column name="sched_name" type="VARCHAR(50)" />
			<column name="TRIGGER_GROUP" type="VARCHAR(50)" />

		</createTable>
	</changeSet>
	<changeSet author="rachna" id="20160414-06">
		<createTable tableName="QRTZ_SCHEDULER_STATE">
			<column name="sched_name" type="VARCHAR(50)" />
			<column name="INSTANCE_NAME" type="VARCHAR(50)" />
			<column name="LAST_CHECKIN_TIME" type="BIGINT" />
			<column name="CHECKIN_INTERVAL" type="BIGINT" />

		</createTable>
	</changeSet>
	<changeSet author="rachna" id="20160414-07">

		<createTable tableName="QRTZ_FIRED_TRIGGERS">
			<column name="sched_name" type="VARCHAR(50)" />
			<column name="ENTRY_ID" type="VARCHAR(150)" />
			<column name="TRIGGER_NAME" type="VARCHAR(50)" />
			<column name="TRIGGER_GROUP" type="VARCHAR(50)" />
			<column name="INSTANCE_NAME" type="VARCHAR(50)" />
			<column name="STATE" type="VARCHAR(50)" />
			<column name="JOB_NAME" type="VARCHAR(50)" />
			<column name="JOB_GROUP" type="VARCHAR(50)" />
			<column name="IS_NONCONCURRENT" type="BIT(1)" />
			<column name="REQUESTS_RECOVERY" type="BIT(1)" />
			<column name="PRIORITY" type="INT" />
			<column name="SCHED_TIME" type="BIGINT" />
			<column name="FIRED_TIME" type="BIGINT" />

		</createTable>
	</changeSet>
	<changeSet author="rachna" id="20160414-08">

		<createTable tableName="QRTZ_SIMPLE_TRIGGERS">
			<column name="sched_name" type="VARCHAR(50)" />
			<column name="TRIGGER_NAME" type="VARCHAR(50)" />
			<column name="TRIGGER_GROUP" type="VARCHAR(50)" />

			<column name="REPEAT_COUNT" type="BIGINT" />
			<column name="REPEAT_INTERVAL" type="BIGINT" />
			<column name="TIMES_TRIGGERED" type="BIGINT" />

		</createTable>
	</changeSet>
</databaseChangeLog>