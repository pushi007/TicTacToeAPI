<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

 	<changeSet author="Dinesh" id="20161128-01">
		<createTable tableName="event" schemaName="moyadev">
			<column name="id" type="CHAR(36)">
				<constraints nullable="false" />
			</column>
			<column name="user_id" type="VARCHAR(50)" />
			<column name="unit_name" type="VARCHAR(50)" />
			<column name="organization_name" type="VARCHAR(50)" />
			<column name="college_name" type="VARCHAR(50)" />
			<column name="event_start_date" type="date" />
			<column name="event_end_date" type="date" />
			<column name="event_title" type="VARCHAR(100)" />
			<column name="description" type="TEXT" />
			<column name="status" type="VARCHAR(20)" />
			<column name="event_mode" type="VARCHAR(50)" />
			<column name="venue" type="VARCHAR(100)" />
			<column name="publish_date" type="date" />
			<column name="rejected_by_level" type="VARCHAR(20)" />
			<column name="created_by" type="VARCHAR(30)" />
			<column name="created_date" type="date" />
			<column name="last_updated" type="VARCHAR(30)" />
			<column name="last_updated_by" type="VARCHAR(30)" />			
		</createTable>
		<addPrimaryKey columnNames="id" tableName="event"
			schemaName="moyadev" />
	</changeSet>                                                        
	
	<changeSet author="Dinesh" id="20161128-02">
		<createTable tableName="achievement" schemaName="moyadev">
			<column name="id" type="CHAR(36)">
				<constraints nullable="false" />
			</column>
			<column name="user_id" type="VARCHAR(50)" />
			<column name="unit_name" type="VARCHAR(50)" />
			<column name="organization_name" type="VARCHAR(50)" />
			<column name="college_name" type="VARCHAR(100)" />
			<column name="achievement_title" type="VARCHAR(100)" />
			<column name="description" type="TEXT" />
			<column name="status" type="VARCHAR(30)" />
			<column name="venue" type="VARCHAR(100)" />
			<column name="publish_date" type="date" />
			<column name="achievement_date" type="date" />
			<column name="rejected_by_level" type="VARCHAR(30)" />
			<column name="created_by" type="VARCHAR(50)" />
			<column name="created_date" type="date" />
			<column name="last_updated" type="VARCHAR(30)" />
			<column name="last_updated_by" type="VARCHAR(50)" />
			<column name="thumb_nail" type="VARCHAR(50)" />						
		</createTable>
		<addPrimaryKey columnNames="id" tableName="achievement"
			schemaName="moyadev" />
	</changeSet>
	
</databaseChangeLog>
