<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
	
	<changeSet author="Jayati" id="20161018-01">
		<createTable tableName="gallery_album" schemaName="moyadev">
			<column name="id" type="CHAR(36)">
				<constraints nullable="false" />
			</column>
			<column name="organization_id" type="VARCHAR(100)" />
			<column name="state" type="VARCHAR(50)" />
			<column name="district" type="VARCHAR(50)" />
			<column name="unit_name" type="VARCHAR(50)" />
			<column name="album_name" type="VARCHAR(50)" />
			<column name="album_thumb" type="VARCHAR(150)" />
			<column name="album_type" type="VARCHAR(10)" />
			<column name="album_description" type="VARCHAR(150)" />
			<column name="created_by" type="VARCHAR(100)" />
			<column name="created_date" type="date" />
			<column name="publish_date" type="VARCHAR(30)" />
			<column name="last_updated" type="timestamp" />
			<column name="last_updated_by" type="VARCHAR(100)" />	
			<column name="status" type="VARCHAR(25)"/>
		</createTable>
		<addPrimaryKey columnNames="id" tableName="gallery_album" schemaName="moyadev" />
	</changeSet>
	
	<changeSet author="Jayati" id="20161018-02">
		<createTable tableName="gallery_items" schemaName="moyadev">
			<column name="id" type="CHAR(36)">
				<constraints nullable="false" />
			</column>
			<column name="parent_id" type="VARCHAR(50)" />
			<column name="item_title" type="VARCHAR(30)" />
			<column name="item_link" type="VARCHAR(150)" />
			<column name="last_updated" type="timestamp" />
			<column name="last_updated_by" type="VARCHAR(100)" />	
			<column name="status" type="VARCHAR(25)"/>
		</createTable>
		<addPrimaryKey columnNames="id" tableName="gallery_items" schemaName="moyadev" />
	</changeSet>
	
	<changeSet author="Dinesh" id="20161118-03">
		<createTable tableName="faq_data" schemaName="moyadev">
			<column name="id" type="CHAR(36)">
				<constraints nullable="false" />
			</column>
			<column name="organization_name" type="VARCHAR(100)" />
			<column name="faq_question" type="TEXT" />			
			<column name="faq_answer" type="TEXT" />
			<column name="publish_date" type="date" />	
			<column name="is_active" type="VARCHAR(30)" />
			<column name="created_by" type="VARCHAR(30)" />
			<column name="created_date" type="date" />			
			<column name="last_updated" type="VARCHAR(30)" />
			<column name="last_updated_by" type="VARCHAR(30)" />
			<column name="last_updated_by" type="integer" >
				<constraints nullable="false" />
			</column>				
		</createTable>
		<addPrimaryKey columnNames="id" tableName="faq_data" schemaName="moyadev" />
	</changeSet>
	
	
</databaseChangeLog>
