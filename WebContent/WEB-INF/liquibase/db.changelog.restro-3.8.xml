<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
	
	<changeSet author="Jayati" id="20161118-01">
		<createTable tableName="activity_log_data" schemaName="moyadev">
			<column name="id" type="CHAR(36)">
				<constraints nullable="false" />
			</column>
			<column name="entity_id" type="VARCHAR(100)" />
			<column name="entity" type="VARCHAR(100)" />
			<column name="error_message" type="VARCHAR(100)" />
			<column name="module_name" type="VARCHAR(100)" />
			<column name="user_id" type="VARCHAR(36)" />
			<column name="username" type="VARCHAR(100)" />
			<column name="ip_address" type="VARCHAR(100)" />
			<column name="browser" type="VARCHAR(36)" />
			<column name="device" type="VARCHAR(100)" />
			<column name="created_date" type="timestamp" />
			<column name="location" type="VARCHAR(100)" />
			<column name="last_updated" type="timestamp" />
			<column name="last_updated_by" type="VARCHAR(100)" />	
			<column name="status" type="VARCHAR(25)"/>
		</createTable>
		<addPrimaryKey columnNames="id" tableName="activity_log_data" schemaName="moyadev" />
	</changeSet>
	
		<changeSet author="jayati" id="20161121-01">
		<addColumn schemaName="moyadev" tableName="menu">
			<column name="menu_name_hindi" type="VARCHAR(100)" />
		</addColumn>
		
	</changeSet>
</databaseChangeLog>
